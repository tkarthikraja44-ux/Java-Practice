import java.util.*;

class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        int row = scanner.nextInt();
        int col = scanner.nextInt();

        int[][] arr = new int[row][col];

        for (int i = 0; i < row; i++) {
            for (int j = 0; j < col; j++) {
                arr[i][j] = scanner.nextInt();
            }
        }

        // List to store lucky numbers
        List<Integer> result = new ArrayList<>();

        // Step 1: For each row
        for (int i = 0; i < row; i++) {

            int min = arr[i][0];
            int minCol = 0;

            // Find min in row
            for (int j = 1; j < col; j++) {
                if (arr[i][j] < min) {
                    min = arr[i][j];
                    minCol = j;
                }
            }

            // Step 2: Check if min is max in its column
            boolean isMax = true;
            for (int k = 0; k < row; k++) {
                if (arr[k][minCol] > min) {
                    isMax = false;
                    break;
                }
            }

            if (isMax) {
                result.add(min);
            }
        }

        // Print result
        System.out.println(result);
    }
}
